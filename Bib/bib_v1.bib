	Date-Added = {2015-06-08 00:05:15 +0000},
	Date-Modified = {2015-06-08 00:06:12 +0000},
	Doi = {10.1051/0004-6361/201321591},
	Eid = {A16},
	Eprint = {1303.5076},
	Journal = {\aap},
	Keywords = {cosmic background radiation, cosmological parameters, early Universe, inflation, primordial nucleosynthesis, neutrino mass},
	Month = nov,
